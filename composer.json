{
    "name": "square360/s360_base_paragraphs",
    "description": "Square360 Base Paragraphs",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "Drupal",
        "Paragraph"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/advanced_header_field": "*",
        "drupal/allowed_formats": "*",
        "drupal/double_field": "*" ,
        "drupal/entity_usage": "*",
        "drupal/field_group": "*",
        "drupal/media_library_edit": "*",
        "drupal/paragraphs": "*",
        "drupal/viewsreference": "*",
        "drupal/webform": "*"
    },
    "require-dev": {
        "drupal/core": "^11",
        "drupal/coder": "^8.3",
        "squizlabs/php_codesniffer": "3.*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "code-sniff": [
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=vendor .",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=vendor ."
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "drupal/core-composer-scaffold": false
        },
        "sort-packages": true
    }
}
